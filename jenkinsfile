@Library('Shared-Library@master') _

pipeline {
  agent any 
	
    stages {
	
		stage('Checkout') {
			steps {
			 gitCheckout(
				branch: "master",
				url: "https://github.com/punnammani/GroovyPractice.git"
				)
			}
		}
		
		stage('Analyze&Report') {
			steps {
			
			sh 'pwd'
			println("$WORKSPACE")

			
			extractValuesFromCSV()
			
			}
		}	
		
	}
	
	post {
  always {
    //emailNotification()
  }
}
}
